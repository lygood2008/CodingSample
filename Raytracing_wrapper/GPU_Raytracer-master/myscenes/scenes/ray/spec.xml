<scenefile>
	<globaldata>
		<diffusecoeff v="0.5"/>
		<specularcoeff v="0.5"/>
		<ambientcoeff v="0.5"/>
	</globaldata>

	<cameradata>
		<pos x="0" y="0" z="3"/>
		<focus x="0" y="0" z="0"/>
		<up x="0" y="1" z="0"/>
		<heightangle v="45"/>
	</cameradata>

	<lightdata>
		<id v="0"/>
		<color r="1" g="1" b="1"/>
		<position x="0" y="0" z="3"/>
	</lightdata>

	<object type="tree" name="box">
		<transblock>
			<translate x="0" y=".8" z="0"/>
			<scale x="1" y=".5" z="1"/>
			<object type="primitive" name="sphere" >
				<reflective r="0" g="0" b="0"/>
				<ambient r=".1" g=".1" b=".1"/>
				<diffuse r="0.4" g="0.4" b="0.4"/>
				<shininess v="20"/>
				<specular r="1.0" g="1.0" b="1.0"/>
				<texture file="/course/cs123/data/image/rabbit.jpg" u="1" v="1"/>
				<blend v="0.5"/>
			</object>
		</transblock>
	</object>

	<object type="tree" name="root">
		<transblock>
			<object type="master" name="box" />
		</transblock>
	</object>

</scenefile>

